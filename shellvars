#!/bin/sh

export SVN_EDITOR=vi
export GDK_NATIVE_WINDOWS=1

export PS1='${debian_chroot:+($debian_chroot)}[\[\033[01;32m\]\h\[\033[00m\]][\[\033[01;34m\]\w\[\033[00m\]]\[\033[0;32m\]`git-branch-prompt`\[\033[0m\]\$ '

case "$TERM" in
xterm*|rxvt*)
    PS1="\[\e]0;${debian_chroot:+($debian_chroot)}\u@\h: \w\a\]$PS1"
    ;;
*)
    ;;
esac


export JAVA_HOME=/data/tools/java/current
export ANT_HOME=/data/tools/apache-ant-1.8.4
export ORACLE_HOME=/data/tools/oracle/11_2

export ANT_HOME=/data/tools/apache-ant-1.8.4
export JAVA_HOME=/data/tools/java/current
export ORACLE_HOME=/data/tools/oracle/11_2
export GROOVY_HOME=/data/tools/groovy-2.3.2
export GRADLE_HOME=/data/tools/gradle-1.12
export SCALA_HOME=/data/tools/scala-2.11.1
export GO_HOME=/data/tools/go

export SQLPATH=$ORACLE_HOME/bin
export LD_LIBRARY_PATH=$ORACLE_HOME/bin